type Team {
    id: ID!,
    teamName: String,
    city: String,
    country: String
    persons: [Person]
}

type Query {
    teams(limit: Int): [Team]
    teamsTest(teamName: String): [Team]
    teamsByCity(city: String): [Team]
    persons: [Person]
}

type Mutation {
    createT(teamName: String, city: String, country: String): Team
}

